# Example passwords file - Copy to passwords.yaml and fill in your values
# DO NOT commit passwords.yaml to version control!

shared:
  # Get your Gemini API key from https://makersuite.google.com/app/apikey
  geminiApiKey: 'YOUR_GEMINI_API_KEY_HERE'

development:
  # These are auto-generated - check your docker-compose.yaml or generate new ones
  database: 'YOUR_DATABASE_PASSWORD'
  redis: 'YOUR_REDIS_PASSWORD'
  serviceSecret: 'YOUR_SERVICE_SECRET'

test:
  database: 'test_db_password'
  redis: 'test_redis_password'

staging:
  database: 'staging_db_password'
  serviceSecret: 'staging_service_secret'

production:
  database: 'production_db_password'
  serviceSecret: 'production_service_secret'
